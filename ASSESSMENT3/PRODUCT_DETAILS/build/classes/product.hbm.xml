<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.model.Product" table="hr.product4">
	
		<id name="productId" type="int" column="ProdId">
			<generator class="assigned"/>
		</id>

		<property name="productName" column="ProdName" />
		<property name="QOH" column="QOH" />
		<property name="price"  />
		

	</class>
</hibernate-mapping>